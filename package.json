{
   "name": "elite-template",
   "version": "0.0.4",
   "description": "This is a template for the Elite Novels.",
   "author": "Sebastian Hilton (https://www.sebastianhilton.com)",
   "homepage": "https://www.elitenovels.com",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/Meeovi/elitenovels-frontend"
   },
   "bugs": {
      "url": "https://github.com/Meeovi/elitenovels-frontend/issues"
   },
   "license": "MIT",
   "private": true,
   "prisma:": {
      "schema": "node_modules/graphql-yoga-prisma/prisma/schema.prisma"
   },
   "scripts": {
      "elite-commit": "git add . && git commit -m \"ready for update\"",
      "elite-update": "git pull \"https://github.com/Meeovi/elitenovels-frontend\"",
      "build": "nuxt build",
      "dev": "nuxt dev --port 3011",
      "ts-node": "ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"CommonJS\"}'",
      "generate": "nuxt generate",
      "preview": "nuxt preview",
      "analyze": "nuxi analyze",
      "postinstall": "nuxt prepare"
   },
   "devDependencies": {
      "@nuxt/content": "^2.3.0",
      "@nuxtjs/apollo": "next",
      "@types/node": "^16.11.10",
      "graphql": "latest",
      "nuxt": "latest",
      "nuxt-meilisearch": "latest",
      "typescript": "latest"
   },
   "dependencies": {
      "@fortawesome/fontawesome-svg-core": "^6.2.1",
      "@fortawesome/free-solid-svg-icons": "^6.2.1",
      "@fortawesome/vue-fontawesome": "^3.0.2",
      "@huntersofbook/naive-ui-nuxt": "^0.5.1",
      "@mdi/font": "^7.1.96",
      "@nuxt/types": "^2.15.8",
      "@sidebase/core": "latest",
      "@sidebase/nuxt-auth": "latest",
      "@storyblok/nuxt": "^5.7.6",
      "@types/graphql": "^14.5.0",
      "graphql-tag": "^2.12.6",
      "instantsearch.css": "^8.0.0",
      "jquery": "latest",
      "keycloak-nuxt-auth-module": "latest",
      "nuxt-directus": "latest",
      "nuxt-security": "^0.10.1",
      "sass": "^1.57.0",
      "ts-node": "^10.9.1",
      "video.js": "^8.0.4",
      "videojs-contrib-ads": "^7.1.0",
      "videojs-contrib-quality-levels": "^3.0.0",
      "videojs-share": "^3.2.1",
      "vue-instantsearch": "^4.8.1",
      "vue-matomo": "latest",
      "vuetify": "latest"
   }
}
